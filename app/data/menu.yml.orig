# Structure
# main_menu:
#     entity_id:
#         name: string to translate. The domain user will be menu
#         route: if it's a link the route name
#         role: role to display the entry. If not set, the rome will be ROLE_ADMIN
#         children : if it's a dropdown
#             entity_id:
#                 name: string to translate. The domain user will be menu
#                 route: route name
#                 role: role to display the entry. If not set, the rome will be ROLE_ADMIN
# right_menu:
#     entity_id:
#         name: string to translate. The domain user will be menu
#         route: if it's a link the route name
#         role: role to display the entry. If not set, the rome will be ROLE_ADMIN
#         children : if it's a dropdown
#             entity_id:
#                 name: string to translate. The domain user will be menu
#                 route: route name
#                 role: role to display the entry. If not set, the rome will be ROLE_ADMIN
main_menu:
    madef_cms_cms:
        name: main_menu.cms
        children:
            madef_cms_cms_page_list:
                name: main_menu.page.list
                route: madef_cms_admin_page_list
                role: ROLE_PAGE_VIEW
            madef_cms_cms_page_edit:
                name: main_menu.page.add
                route: madef_cms_admin_page_add
                role: ROLE_PAGE_EDIT
            madef_cms_cms_layout_list:
                name: main_menu.layout.list
                route: madef_cms_admin_layout_list
                role: ROLE_LAYOUT
            madef_cms_cms_layout_edit:
                name: main_menu.layout.add
                route: madef_cms_admin_layout_add
                role: ROLE_LAYOUT
            madef_cms_cms_widget_list:
                name: main_menu.widget.list
                route: madef_cms_admin_widget_list
                role: ROLE_WIDGET
            madef_cms_cms_widget_edit:
                name: main_menu.widget.add
                route: madef_cms_admin_widget_add
                role: ROLE_WIDGET
            madef_cms_cms_media_list:
                name: main_menu.media.list
                route: madef_cms_admin_media_list
                role: ROLE_MEDIA
            madef_cms_cms_media_edit:
                name: main_menu.media.add
                route: madef_cms_admin_media_add
                role: ROLE_MEDIA
            madef_cms_cms_version_list:
                name: main_menu.version.list
                route: madef_cms_admin_version_list
                role: ROLE_VERSION_EDIT
            madef_cms_cms_version_edit:
                name: main_menu.version.add
                route: madef_cms_admin_version_add
                role: ROLE_VERSION_EDIT
right_menu:
    madef_cms_parameters:
        name: right_menu.parameters
        children:
            madef_cms_parameters_account:
                name: right_menu.myaccount
                route: fos_user_profile_edit
            madef_cms_parameters_users:
                name: right_menu.users
                route: madef_cms_admin_user_list
                role: ROLE_SUPER_ADMIN
    madef_cms_logout:
        name: right_menu.logout
        route: fos_user_security_logout
